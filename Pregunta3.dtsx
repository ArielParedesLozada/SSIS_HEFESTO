          SQLTask:SqlStatementSource="-- Paso 1: calcular valores agregados globales&#xA;DECLARE @TotalCanceladasRechazadas INT;&#xA;DECLARE @SumaCanceladasRechazadas MONEY;&#xA;DECLARE @TotalEnviadas INT;&#xA;&#xA;SELECT &#xA;    @TotalCanceladasRechazadas = COUNT(*)&#xA;FROM AdventureWorks2022.Sales.SalesOrderHeader&#xA;WHERE Status IN (4, 6);  -- Rechazadas y canceladas&#xA;&#xA;SELECT &#xA;    @SumaCanceladasRechazadas = SUM(TotalDue)&#xA;FROM AdventureWorks2022.Sales.SalesOrderHeader&#xA;WHERE Status IN (4, 6);&#xA;&#xA;SELECT &#xA;    @TotalEnviadas = COUNT(*)&#xA;FROM AdventureWorks2022.Sales.SalesOrderHeader&#xA;WHERE Status = 5;&#xA;&#xA;-- Paso 2: insertar en FactÓrdenes&#xA;INSERT INTO Hefesto.dbo.FactÓrdenes (&#xA;    IDOrdenVenta,&#xA;    TotalPagado,&#xA;    IDEmpleado,&#xA;    ClaveFechaEnvío,&#xA;    CantidadOrdenesCanceladasRechazadas,&#xA;    SumaTotalCanceladasRechazadas,&#xA;    PorcentajeCanceladasRechazadas&#xA;)&#xA;SELECT&#xA;    SOH.SalesOrderID,&#xA;    SOH.TotalDue,&#xA;    SOH.SalesPersonID,&#xA;    CONVERT(INT, FORMAT(SOH.ShipDate, 'yyyyMMdd')),&#xA;    @TotalCanceladasRechazadas,&#xA;    @SumaCanceladasRechazadas,&#xA;    CAST(@TotalCanceladasRechazadas AS FLOAT) / NULLIF(@TotalEnviadas, 0)&#xA;FROM AdventureWorks2022.Sales.SalesOrderHeader AS SOH&#xA;WHERE SOH.Status = 5&#xA;  AND SOH.ShipDate IS NOT NULL&#xA;  AND SOH.SalesPersonID IS NOT NULL&#xA;  AND EXISTS (&#xA;      SELECT 1 FROM Hefesto.dbo.DimEmpleados e&#xA;      WHERE e.IDEmpleado = SOH.SalesPersonID&#xA;  )&#xA;  AND EXISTS (&#xA;      SELECT 1 FROM Hefesto.dbo.DimTiempo t&#xA;      WHERE t.ClaveFecha = CONVERT(INT, FORMAT(SOH.ShipDate, 'yyyyMMdd'))&#xA;  );&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
